        <OrganizationName>
          <r:String xml:lang="en-GB">Medicals Direct Group Ltd</r:String>
        </OrganizationName>
      </OrganizationIdentification>
      <r:Description>
        <r:Content xml:lang="en-GB">MDG was founded in 1994 and provides medical data gathering services to the life insurance market, and specialist reports and rehabilitation services to support claims services (&lt;a href="https://www.medicalsdirectgroup.co.uk" target="_blank" &gt;MDG, n.d.&lt;/a&gt;).</r:Content>
