      <r:Citation>
        <r:Title>
          <r:String xml:lang="en-GB">Wirral Child Health and Development Study</r:String>
        </r:Title>
        <r:AlternateTitle>
          <r:String xml:lang="en-GB">First Steps Study</r:String>
        </r:AlternateTitle>
        <r:Creator>
          <r:CreatorName>
            <r:String xml:lang="en-GB">Professor Jonathan Hill (University of Reading), Professor Helen Sharp  (University of Liverpool)</r:String>
          </r:CreatorName>
        </r:Creator>
        <r:Publisher>
